{
  "name": "cms",
  "version": "1.0.0",
  "description": "Verscienta Health Payload CMS",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3001",
    "build": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3001",
    "generate:types": "payload generate:types",
    "generate:importmap": "payload generate:importmap",
    "payload": "payload"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.62.0",
    "@payloadcms/next": "^3.62.0",
    "@payloadcms/plugin-cloud-storage": "^3.62.0",
    "@payloadcms/plugin-sentry": "^3.62.0",
    "@payloadcms/richtext-lexical": "^3.62.0",
    "@payloadcms/ui": "^3.62.0",
    "payload": "^3.62.0",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "graphql": "^16.9.0",
    "algoliasearch": "^5.18.0",
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.20.2"
  }
}
