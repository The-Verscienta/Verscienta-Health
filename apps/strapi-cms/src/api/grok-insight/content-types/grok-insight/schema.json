{
  "kind": "collectionType",
  "collectionName": "grok_insights",
  "info": {
    "singularName": "grok-insight",
    "pluralName": "grok-insights",
    "displayName": "Grok Insight",
    "description": "AI-generated health insights from Grok"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "input": {
      "type": "json"
    },
    "analysis": {
      "type": "richtext"
    },
    "recommendations": {
      "type": "component",
      "component": "grok.recommendation",
      "repeatable": true
    },
    "followUpQuestions": {
      "type": "component",
      "component": "grok.follow-up-question",
      "repeatable": true
    },
    "disclaimers": {
      "type": "richtext"
    },
    "relatedHerbs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::herb.herb"
    },
    "relatedConditions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::condition.condition"
    },
    "sessionId": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "grokModel": {
      "type": "string",
      "default": "grok-beta"
    },
    "tokensUsed": {
      "type": "integer"
    }
  }
}
