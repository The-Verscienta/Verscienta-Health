{
  "kind": "collectionType",
  "collectionName": "symptoms",
  "info": {
    "singularName": "symptom",
    "pluralName": "symptoms",
    "displayName": "Symptom",
    "description": "Health symptoms and their interpretations"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "physical",
        "mental",
        "digestive",
        "respiratory",
        "pain",
        "skin",
        "sleep",
        "energy",
        "other"
      ]
    },
    "severity": {
      "type": "json"
    },
    "duration": {
      "type": "json"
    },
    "relatedConditions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::condition.condition"
    },
    "relatedHerbs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::herb.herb"
    },
    "tcmInterpretation": {
      "type": "richtext"
    },
    "westernInterpretation": {
      "type": "richtext"
    },
    "redFlags": {
      "type": "richtext"
    },
    "commonCauses": {
      "type": "component",
      "repeatable": true,
      "component": "symptom.common-cause"
    }
  }
}
